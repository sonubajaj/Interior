<!--#?container-->
<div id="container" class="content-container">
    <div class="row int-container">
        <section class="list-base">
            <div class="container">
                <div class="row">
                    <ol class="breadcrumb">
                        <li><a href="">Home</a></li>
                        {{#if Promo}}
                        <li><a href="">Promo</a></li>
                        {{else}}
                        <li><a href="">Store</a></li>
                        {{#if CollectionId}}
                        <li><a href="/brand:{{CollectionId}}">{{CollectionId}}</a></li>
                        {{else}}
                        <li><a href="/{{Cat_header}}">{{Cat_header}}</a></li>
                        <li class="active"><a href="">{{MapCat}}</a></li>
                        {{/if}}
                        {{/if}}
                    </ol>
                </div>
                <div id="filters-and-results">
                    <div id="dvnodata" style="display:none" class="col-md-3 col-sm-4 tag-lt-base hidden-xs">
                        No Data Found For Filters
                    </div>
                    {{{filters}}}
                    <div id="results-and-pagination" class="col-md-9 col-sm-8 col-xs-12 tag-rt-base">
                        <div id="header-sort-and-selected-filters" class="row main-base">
                            <div class="row phomelink hidden">
                                <div class="col-sm-4 col-xs-12">
                                    <a id="ancHomelnk" title="HOME" href="/">Home</a>
                                    <a id="ancHomelnkhead" title="" class="tt"> / {{Cat_header}}</a>
                                </div>
                            </div>
                            <div class="row phomelink1">
                                <h4 id="hdHead">{{{results_header}}}<span></span></h4>
                                <div class="row">
                                    <div class="col-sm-4 fdesign1">
                                        <p id="spnfoundmsg">( {{TotalRecords}} )</p>
                                    </div>
                                </div>
                            </div>
                            <div class="row mfilter-base visible-xs">
                                <div class="row flt-bt">
                                    <div class="col-xs-6"><a id="ShowFilter"><i class="fa fa-filter"></i>FILTER</a></div>
                                    <div class="col-xs-6 pull-right"><a href="#"><i class="fa fa-sort"></i>SORT BY</a></div>
                                </div>
                            </div>
                            {{{sort}}}
                        </div>
                        <div style="color:gray; margin-bottom:15px;">{{{selected_filters}}}</div>
                        {{{results}}}

                        <div class="clearfix"></div>
                        <div id="load_more">
                            <a href="" title="Click to load more..." style="cursor:default">Click to load more...</a>
                        </div>
                        <div id="loading_to_append" title="Loading more..." style="cursor:default">Loading more...</div>
                        <div id="end_of_search" title="No more to load" style="cursor:default">No more to load</div>
                        {{{pagination_html}}}
                    </div>
                </div>
            </div>
            <!--#?end-->
            <!--#?results-->
            <div>
                <div class="results_page">
                </div>
                <h6 style="display:none">Page {{page_number}} of {{total_pages}}</h6>
                <div class="products" id="dvPMain">
                    <div class="row exp-base">
                        <div class="row exp-pd-base">
                            <ul class="newsListClass" data-page="listing" id="results">
                                {{{results_page}}}
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <!--#?end-->
            <!--#?no_results-->
            <div id="results" class="no_results">
                {{#if multiple_alternatives}}

                {{{alternatives}}}
                {{else}}
                <div class="products">
                    {{{products}}}
                </div>
                {{/if}}
            </div>
            <!--#?end-->
            <!--#?results_header-->
            <div id="results_header">
                <h4>Search results for "{{query}}"</h4>
                <div class="row">
                    <div style="display:none" class="col-sm-4 fdesign1">
                        <p id="number_of_results">FOUND {{number_of_results}} {{pluralise number_of_results 'DESIGN'}}</p>
                    </div>
                </div>
            </div>
            <!--#?end-->
            <!--#?no_results_header-->
            <div id="no_results_header">
                {{{heading}}}
            </div>
            <!--#?end-->
            <!--#?empty_query_heading-->
            <h3>Please enter a search term</h3>
            <!--#?end-->
            <!--#?no_results_heading-->
            <h4>No results for "{{query}}"</h4>
            <!--#?end-->
            <!--#?no_results_heading_with_alternative-->
            <h4>No results for "{{query}}". Maybe you're interested in "{{alternative_query}}"?</h4>
            <a href="{{alternative_query_search_link}}">View all ({{alternative_query_results_count}})</a>
            <!--#?end-->
            <!--#?no_results_heading_with_multiple_alternatives-->
            <h4>No results for "{{query}}". Maybe you're interested in...</h4>
            <!--#?end-->
            <!--#?each_alternative-->
            <div class="alternatives">
                {{{alternative_header}}}
                <div class="products">
                    {{{alternative_products}}}
                </div>
            </div>
            <!--#?end-->
            <!--#?each_alternative_header-->
            <div class="alternative_header">
                <h4>{{alternative_query}}</h4>
                <a href="{{alternative_query_search_link}}">View all ({{alternative_query_results_count}})</a>
            </div>
            <!--#?end-->
            <!--#?selected_filters-->
            <ul id="selected_filters" class="s-filter">
                {{#each filters}}
                {{#equal type 'checkbox'}}
                {{#each items}}
                {{#if selected}}
                <li>
                    <a href="" data-clear-filter-name="{{../../prefix}}" data-clear-filter-value="{{id}}">
                        {{name}}<span class="clear-that">&nbsp;&nbsp;x</span>
                    </a>
                </li>
                {{/if}}
                {{/each}}
                {{/equal}}
                {{#equal type 'range'}}
                {{#if selected_max}}
                {{#equal ../../prefix 'p'}}
                <li>
                    <a href="" data-clear-filter-name="{{../../prefix}}">
                        {{indian_currency_format selected_min}} - {{indian_currency_format selected_max}}<span class="clear-that">&nbsp;&nbsp;x</span>
                    </a>
                </li>
                {{/equal}}
                {{#equal ../../prefix 'd'}}
                <li>
                    <a href="" data-clear-filter-name="{{../../prefix}}">
                        {{selected_min}}% - {{selected_max}}% {{../../name}}<span class="clear-that">&nbsp;&nbsp;x</span>
                    </a>
                </li>
                {{/equal}}
                {{/if}}
                {{/equal}}
                {{/each}}
            </ul>
            <!--#?end-->
            <!--#?results_page-->
            {{{products}}}
            <!--#?end-->
            <!--#?product_tile-->
