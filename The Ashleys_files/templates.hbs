            <!--#?end-->
            <!--#?filters-->
            <div class="col-md-3 col-sm-4 tag-lt-base hidden-xs" id="resultsContainer" style="opacity: 1;">
                <div id="filters">
                    <div id="filtersContainer">
                        <h5>Filter By</h5>
                        {{#each filters}}
                        {{#equal type 'checkbox'}}
                        <div id="filter-{{prefix}}" class="filter checkboxes collapsible styled-scrollbars collapsed">
                            <h6>
                                <span class="toggle"></span>
                                <span class="title">{{name}}</span>
                            </h6>
                            <div class="filter-filter-and-items fsize">
                                <input type="text" class="filter-filter" placeholder="Search {{name}}"/>
                                <div class="row tag-list">
                                    <ul>
                                        {{#each items}}
                                        <li>
                                            <label>

                                                <input type="checkbox" style="margin-right: 6px;" name="{{../prefix}}" value="{{id}}" {{#if selected}} checked="checked" {{/if}}><span class="name">{{name}}</span>{{#if count}} <span class="count">({{count}})</span>{{/if}}

                                            </label>
                                        </li>
                                        {{/each}}
                                    </ul>
                                </div>
                            </div>
                        </div>
                        {{/equal}}
                        {{#equal type 'range'}}
                        <div id="filter-{{prefix}}" class="filter range collapsible collapsed">
                            <h6>
                                <span class="toggle"></span>
                                <span class="title">{{name}}</span>
                            </h6>
                            <div class="filter-filter-and-items">
                                <input type="hidden" name="{{prefix}}f" data-prefix="{{prefix}}" data-subdivision="min" data-original-min="{{#if_defined original_min}}{{original_min}}{{else}}{{min}}{{/if_defined}}" id="filter-{{prefix}}-f" value=""></input>
                                <input type="hidden" name="{{prefix}}t" data-prefix="{{prefix}}" data-subdivision="max" data-original-max="{{#if_defined original_max}}{{original_max}}{{else}}{{max}}{{/if_defined}}" id="filter-{{prefix}}-t" value=""></input>
                                <span id="filter-{{prefix}}-initial-min-label" class="initial-min">{{#equal name 'price'}}{{currency}}{{/equal}}{{#if_defined original_min}}{{original_min}}{{else}}{{min}}{{/if_defined}}{{#equal name 'discount'}}%{{/equal}}</span>
                                <span id="filter-{{prefix}}-initial-max-label" class="initial-max">{{#equal name 'price'}}{{currency}}{{/equal}}{{#if_defined original_max}}{{original_max}}{{else}}{{max}}{{/if_defined}}{{#equal name 'discount'}}%{{/equal}}</span>
                                <div class="slider" id="slider-{{prefix}}"></div>
                                <div class="current-min-and-max">
                                    <span id="filter-{{prefix}}-current-min-label" class="current-min"></span> to <span id="filter-{{prefix}}-current-max-label" class="current-max"></span>
                                </div>
                            </div>
                        </div>
                        {{/equal}}
                        {{/each}}
                    </div>
                </div>
            </div>
            <!--#?end-->
            <!--#?sort-->
            <div class="sort1">
                <div id="sort" class="dropdownable1 hidden-xs">
                    <span>
                        <span class="label">sort: {{sort_string sort}}</span>
                    </span>
                    <ul>
                        {{#each sortOptions}}
                        <li>
                            <a href="" class="sort-link" data-sort="{{this}}">{{sort_string this}}</a>
                        </li>
                        {{/each}}
                    </ul>
                </div>
            </div>
            <!--#?end-->
            <!--#?pagination-->
            {{#pagination page per_page total}}
            {{#if pagination}}
            <ul id="pagination">
                <li class="full">
                    <span>Page {{currentPage}} of {{totalPages}}</span>
                </li>
                {{#unless startFromFirstPage}}
                <li>
                    <a href="" data-tagalys-to-page="1" class="page-link">1{{#unless startFromSecondPage}}...{{/unless}}</a>
                </li>
                {{/unless}}

                {{#each pages}}
                {{#if isCurrent}}
                <li>
                    <span class="current">{{page}}</span>
                </li>
                {{/if}}
                {{#unless isCurrent}}
                <li>
                    <a href="" data-tagalys-to-page="{{page}}" class="page-link">{{page}}</a>
                </li>
                {{/unless}}
                {{/each}}

                {{#unless endAtLastPage}}
                <li>
                    <span>...</span>
                </li>
                {{/unless}}
            </ul>
            {{/if}}
            {{/pagination}}
            <!--#?end-->
            <!--#?mobilefiltersTitle-->
            <div class="row mflt-title" id="MobileFilterTitle"><b>FILTER</b> {{TotalRecords}}</div>
            <!--#?end-->
            <!--#?mobilefilters-->
            <div class="row mflt-ct-base" id="mobileFilterContianer">
                <div class="col-xs-4 mflt-lt-base">
                    <ul id="MobileFilterList">
                        {{#each filters}}
                        {{#equal type 'checkbox'}}
                        <li><a data-filtername="{{name}}" {{#if @index}} {{else}} class="active" {{/if}}> {{name}}</a></li>
                        {{/equal}}
                        {{/each}}
                    </ul>
                </div>
                <div class="col-xs-7 mflt-rt-base">
                    <div class="row search hidden">
                        <div class="col-xs-1"><i class="fa fa-search" style="padding: 0px;"></i></div>
                        <div class="col-xs-10">
                            <input type="text" name="textfield" id="textfield" />
                        </div>
                        <div class="col-xs-1 text-right"><a href="#"><i class="fa fa-times" style="padding: 0px;"></i></a></div>
                        <div class="clearfix"></div>
                    </div>
                    <div class="row mlist-base MobileFilterListDetails">
                        {{#each filters}}
                        {{#equal type 'checkbox'}}
                        <ul data-filtername="{{name}}" {{#if @index}} {{else}} class="active" {{/if}}>
                            {{#each items}}
                            <li><a {{#if selected}} class="active" {{/if}}>{{name}} {{#if count}}({{count}}){{/if}}<span class="pull-right"><input type="checkbox" name="{{../prefix}}" value="{{id}}" {{#if selected}} checked="checked" {{/if}} /></span></a></li>
                            {{/each}}
                        </ul>
                        {{/equal}}
                        {{/each}}
                    </div>
                </div>
                <div class="clearfix"></div>
                <div class="row mflt-bt">
                    <div class="col-xs-6">
                        <a class="bt-3" id="MobileClearFilters">CLEAR FILTERS</a>
                    </div>
                    <div class="col-xs-6">
                        <a class="bt-4" id="MobileApplyFilters">APPLY</a>
                    </div>
                </div>
            </div>
            <!--#?end-->
        </section>
    </div>
</div>


